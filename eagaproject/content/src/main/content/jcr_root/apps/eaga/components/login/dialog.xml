<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          title="Login"
          xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <indirizzoMailNumero
            name="./indirizzoMailNumero"
            jcr:primaryType="cq:Widget"
            fieldLabel="Indirizzo e-mail o numero di cellulare"
            allowBlank="false"
            defaultValue="Indirizzo e-mail o numero di cellulare"
            xtype="textfield"/>
        <labelPassword
            name="./labelPassword"
            jcr:primaryType="cq:Widget"
            fieldLabel="Password"
            allowBlank="false"
            defaultValue="Password"
            xtype="textfield"/>
        <labelPasswordDimenticata
            name="./labelPasswordDimenticata"
            jcr:primaryType="cq:Widget"
            fieldLabel="Password Dimenticata"
            allowBlank="false"
            defaultValue="Password Dimenticata"
            xtype="textfield"/>
         <labelSeiNuovo
            name="./labelSeiNuovo"
            jcr:primaryType="cq:Widget"
            fieldLabel="Label Sei nuovo"
            allowBlank="false"
            defaultValue="----Sei nuovo su Amazon?----"
            xtype="textfield"/>
          <labelButtonAccedi
            name="./labelButtonAccedi"
            jcr:primaryType="cq:Widget"
            fieldLabel="Accedi"
            allowBlank="false"
            defaultValue="Accedi"
            xtype="textfield"/>
          <labelButtonCrea
            name="./labelButtonCrea"
            jcr:primaryType="cq:Widget"
            fieldLabel="Crea il tuo account Amazon"
            allowBlank="false"
            defaultValue="Crea il tuo account Amazon"
            xtype="textfield"/>
    </items>
</jcr:root>
