        <cart
            name="./cartIcon"
            jcr:primaryType="cq:Widget"
            width="500"
            fieldLabel="Cart Icon"
            rootPath="/etc/designs/EAGA-Design/images"
            xtype="pathfield" />
    </items>
</jcr:root>
