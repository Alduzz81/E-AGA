<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          title="Login"
          xtype="panel">
	    <items jcr:primaryType="cq:WidgetCollection">
	    	<logo
	            name="./logo"
	            jcr:primaryType="cq:Widget"
	            fieldLabel="Logo Path"
	            allowBlank="false"
	            rootPath="/etc/designs/EAGA-Design/images"
	            value="/etc/designs/EAGA-Design/images/eaga-logo-login.png"
	            defaultValue="/etc/designs/EAGA-Design/images/eaga-logo-login.png"
	            xtype="pathfield" />
	    	<labelAccedi
	            name="./labelAccedi"
	            jcr:primaryType="cq:Widget"
	            fieldLabel="Label Accedi"
	            allowBlank="false"
	            defaultValue="Accedi"
	            xtype="textfield"/>
	        <labelIndirizzoMail
	            name="./labelIndirizzoMail"
	            jcr:primaryType="cq:Widget"
	            fieldLabel="Label Indirizzo e-mail"
	            allowBlank="false"
	            defaultValue="Indirizzo e-mail"
	            xtype="textfield"/>
	        <labelPassword
	            name="./labelPassword"
	            jcr:primaryType="cq:Widget"
	            fieldLabel="Label Password"
	            allowBlank="false"
	            defaultValue="Password"
	            xtype="textfield"/>
	        <labelPasswordDimenticata
	            name="./labelPasswordDimenticata"
	            jcr:primaryType="cq:Widget"
	            fieldLabel="Label Password Dimenticata"
	            allowBlank="false"
	            defaultValue="Password Dimenticata"
	            xtype="textfield"/>
	         <labelSeiNuovo
	            name="./labelSeiNuovo"
	            jcr:primaryType="cq:Widget"
	            fieldLabel="Label Sei nuovo su E-AGA"
	            allowBlank="false"
	            defaultValue="----Sei nuovo su E-AGA?----"
	            xtype="textfield"/>
	          <labelButtonAccedi
	            name="./labelButtonAccedi"
	            jcr:primaryType="cq:Widget"
	            fieldLabel="Label Button Accedi"
	            allowBlank="false"
	            defaultValue="Accedi"
	            xtype="textfield"/>
	          <labelButtonCrea
	            name="./labelButtonCrea"
	            jcr:primaryType="cq:Widget"
	            fieldLabel="Label Button Crea il tuo account E-AGA"
	            allowBlank="false"
	            defaultValue="Crea il tuo account E-AGA"
	            xtype="textfield"/>
	    </items>
</jcr:root>
